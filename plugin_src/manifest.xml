<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin certificationLevel="None" displayName="IIQuery" minSystemVersion="8.1" name="IIQuery" version="3.1">
  <Attributes>
    <Map>
      <entry key="minUpgradableVersion" value="2.0" />
      <entry key="fullPage">
        <value>
          <FullPage title="Todo List" />
        </value>
      </entry>
      <entry key="restResources">
        <value>
          <List>
            <String>nl.a11n.sailpoint.Main</String>
          </List>
        </value>
      </entry>
      <entry key="settings">
        <value>
          <List>
            <Setting dataType="boolean" helpText="TodoPlugin_setting_delete_allowed_help_text" label="TodoPlugin_setting_delete_allowed_label" name="canDelete" defaultValue="true" />
            <Setting dataType="string" helpText="TodoPlugin_setting_default_name_help_text" label="TodoPlugin_setting_default_name_label" name="defaultName" defaultValue="My Todo" />
            <Setting dataType="int" helpText="TodoPlugin_setting_default_time_help_text" label="TodoPlugin_setting_default_time_label" name="defaultTime" defaultValue="30" />
            <Setting dataType="int" helpText="TodoPlugin_setting_max_active_help_text" label="TodoPlugin_setting_max_active_label" name="maxUntilFlagged" defaultValue="10" />
          </List>
        </value>
      </entry>
      <entry key="snippets">
        <value>
          <List>
            <Snippet regexPattern=".*">
              <Scripts>
                <String>ui/js/snippets/test.js</String>
              </Scripts>
            </Snippet>
          </List>
        </value>
      </entry>
    </Map>
  </Attributes>
</Plugin>
